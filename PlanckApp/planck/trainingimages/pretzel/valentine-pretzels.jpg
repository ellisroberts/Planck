	<script>
	$(function() {

		// Home page slider
		var homeSlider = $('.home-slider .slide-container').flickity({
			cellAlign: 'left',
			contain: true,
			prevNextButtons: true,
			pageDots: true,
			autoPlay: 5000,
			wrapAround: true,
			imagesLoaded: true
		});

		// Products grid
		var productsGrid = $('.products-grid .grid-container').isotope({
			itemSelector: '.grid-item',
			layoutMode: 'packery',
			packery: {
				gutter: 0,
				percentPosition: true
			}
		});

		// Products carousel
		var productsCarousel = $('.products-carousel .carousel-container').flickity({
			cellAlign: 'left',
			contain: true,
			prevNextButtons: true,
			pageDots: false,
			autoPlay: true,
			wrapAround: true
		});

		// Home page recipe highlights
		$('.recipe-highlights [data-recipe-nav]').each(function(index, nav) {
			var $nav = $(nav);

			$nav.on('click', function(e) {
				e.preventDefault();

				var recipeID = $nav.attr('data-recipe-nav');

				$('.recipe-highlights > [data-recipe-bg]').css("background-image", 'url(' + $nav.attr('data-recipe-bg') + ')');

				$('.recipe-highlights [data-recipe-nav]').removeClass('active');
				$('.recipe-highlights [data-recipe-nav=' + recipeID + ']').addClass('active');

				$('.recipe-highlights [data-recipe-romance]').removeClass('active');
				$('.recipe-highlights [data-recipe-romance=' + recipeID + ']').addClass('active');
			});
		});

	});
</script>

