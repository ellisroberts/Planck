		timer = setTimeout(eval("button1_click"),"7000");
		slide = 3;
		$("#image3").animate({"opacity":1},300, "linear", null);
		}
		
		$("#leftclick").click(function ()
		{
		slide = slide-1;
		if (slide = 1){
			button1_click(event);
		} else if (slide = 2){
			button2_click(event);
		} else {
			button3_click(event);
			slide = 3;
		}
		});
		
		$("#rightclick").click(function ()
		{
		slide = slide+1;
		if (slide == "2"){
			button2_click(event);
